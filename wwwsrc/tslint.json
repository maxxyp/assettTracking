{
  "rulesDirectory": "./build/tslint/",
  "rules": {
    "ban": [
      "function"
    ],
    "class-name": true,
    "comment-format": [
      true,
      "check-lowercase",
      "check-space"
    ],
    "curly": true,
    "eofline": true,
    "forin": false,
    "indent": [
      true,
      "spaces"
    ],
    "interface-name": true,
    "label-position": true,
    "max-line-length": [
      true,
      200
    ],
    "member-access": true,
    "member-ordering": [
      true,
      {
        "order": "fields-first"
      }
    ],
    "no-any": false,
    "no-arg": true,
    "no-bitwise": false,
    "no-conditional-assignment": true,
    "no-console": [
      true,
      "log",
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "no-consecutive-blank-lines": true,
    "no-construct": true,
    "no-parameter-properties": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty": false,
    "no-eval": true,
    "no-internal-module": true,
    "no-require-imports": true,
    "no-shadowed-variable": true,
    "no-string-literal": true,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": false,
    "no-unused-expression": true,
    "no-unused-variable": false,
    "no-var-keyword": true,
    "one-line": [
      true,
      "check-open-brace",
      "check-catch",
      "check-else",
      "check-whitespace"
    ],
    "quotemark": [
      true,
      "double"
    ],
    "radix": true,
    "semicolon": true,
    "object-literal-sort-keys": false,
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "typedef": [
      true,
      "call-signature",
      "property-declaration",
      "member-variable-declaration"
    ],
    "typedef-whitespace": [
      true,
      "call-signature",
      "parameter",
      "property-declaration",
      "variable-declaration",
      "member-variable-declaration"
    ],
    "variable-name": [
      true,
      "check-format",
      "allow-leading-underscore",
      "ban-keywords"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ],
    "accessor-after-constructor": true,
    "ban-functions": [
      true,
      "alert",
      "confirm",
      "prompt",
      "setTimeout",
      "clearTimeout",
      "setInterval",
      "clearInterval"
    ],
    "export-name": [
      true,
      "configure"
    ],
    "method-name": true,
    "no-delete-expression": true,
    "no-function-expression": true,
    "no-invalid-array-declaration": true,
    "no-invalid-import-paths": true,
    "no-missing-visibility-modifiers": true,
    "no-property-assignments": true,
    "no-unnecessary-bind": true,
    "no-unnecessary-field-initialization": true,
    "no-unnecessary-local-variable": true,
    "no-unnecessary-semicolons": true,
    "private-property-underscore": true,
    "promise-must-complete": true
  }
}